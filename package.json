{
  "name": "www.velobipede.fr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "predeploy": "shx rm -rf ./.dist && shx mkdir ./.dist/",
    "deploy": "next build && pnpm run copy-standalone && pnpm run copy-public && pnpm run copy-static",
    "copy-standalone": "shx cp -RfL .next/standalone/ .dist/",
    "copy-public": "copyfiles -a -V public/**/* .dist/",
    "copy-static": "copyfiles -a .next/static/**/* .dist/",
    "run_deploy": "node ./.dist/server.js"
  },
  "dependencies": {
    "@conform-to/react": "^1.8.2",
