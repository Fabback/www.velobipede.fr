    "@mantine/form": "^8.2.7",
    "@mantine/hooks": "^8.2.7",
    "@mantine/modals": "^8.2.7",
    "@mantine/notifications": "^8.2.7",
    "@tabler/icons-react": "^3.34.1",
    "@tailwindcss/postcss": "^4.1.12",
    "dayjs": "^1.11.13",
    "embla-carousel-react": "^8.6.0",
    "next": "^15.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^19.1.1",
